            <connectionFormat>CF_GENIVI_STEREO</connectionFormat>
            <availability>
                <availability>A_AVAILABLE</availability>
                <availabilityReason>AR_GENIVI_NEWMEDIA</availabilityReason>
            </availability>
            <soundProperty>
